#!/bin/bash
JOBNAME=CaMgO_full_tmp_650
OUTDIR=/mnt/lustre/a2fs-work2/work/e05/e05/bcamino/descriptors/full_opt
OUTPUT=CaMgO_full_tmp_650.out
